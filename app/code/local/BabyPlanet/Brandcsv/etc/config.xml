<?xml version="1.0"?>
<config>
    <modules>
        <BabyPlanet_Brandcsv>
            <version>0.1.0</version>
        </BabyPlanet_Brandcsv>
    </modules>
    <admin>
        <routers>
            <brandcsv>
                <use>admin</use>
                <args>
                    <module>BabyPlanet_Brandcsv</module>
                    <frontName>brandcsv</frontName>
                </args>
            </brandcsv>
        </routers>
    </admin>
    <adminhtml>
        <acl>
            <resources>
                <all>
                    <title>Allow Everything</title>
                </all>
                <admin>
                    <children>
                        <brandcsv>
                            <title>Brandcsv Module</title>
                            <sort_order>200</sort_order>
                        </brandcsv>
                    </children>
                </admin>
            </resources>   
        </acl>
        <layout>
            <updates>
                <brandcsv>
                    <file>brandcsv.xml</file>
                </brandcsv>
            </updates>
        </layout>
    </adminhtml>   
    <global>
        <helpers>
            <brandcsv>
                <class>BabyPlanet_Brandcsv_Helper</class>
            </brandcsv>
        </helpers>
    </global>
</config>
